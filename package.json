    "react-intersection-observer": "^9.4.3",
    "react-spring": "^9.7.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "vite": "^5.4.4",
    "vitest": "^0.34.6",
